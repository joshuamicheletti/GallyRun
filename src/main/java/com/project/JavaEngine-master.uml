            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_BQyrFt9lEeyaYIzP8fvkgQ" name="calculateBoundingBox" specification="_A-TM4N9lEeyaYIzP8fvkgQ">
