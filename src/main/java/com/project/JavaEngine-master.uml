              <language>java</language>
              <body>if (alcGetString(0, ALC_DEFAULT_DEVICE_SPECIFIER) != null) {&#xD;
			int state = alGetSourcei(this.sourceID, AL_SOURCE_STATE);&#xD;
			if (state == AL_STOPPED) {&#xD;
